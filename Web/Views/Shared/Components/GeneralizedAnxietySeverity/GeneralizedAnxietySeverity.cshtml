                        <div onclick="document.getElementById('@(Html.IdFor(_ => Model.UserMood.Fright))').value = document.getElementById('@(Html.IdFor(_ => Model.UserMood.Fright))-input').value = this.dataset.value" data-value="1" style="white-space:nowrap;text-decoration:underline;cursor:pointer;position:absolute;left:calc(20% - .5ch)">Poor</div>
