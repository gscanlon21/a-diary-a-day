@using Data.Entities.User
@using Lib.Pages.Shared.Exercise

<h6 class="mb-1">DSM-5-TR Generalized Anxiety Severity</h6>
<div class="alert alert-light shadow">
    @if (Model.PreviousMood == null || Model.PreviousMood.Date < Core.Dates.Today.AddDays(-7))
